import math
import random

x = random.randint(1000, 9999)
c = str(x)
w = c[0]
e = str(x)
r = e[1]
p = str(x)
f = p[2]
k = str(x)
z = k[3]
b = (int(w)+int(r)+int(f)+int(z))
o = random.randint(10000, 99999)
t = str(o)
i = t[0]
y = str(o)
q = y[1]
a = str(o)
s = a[2]
d = str(o)
g = d[3]
h = str(o)
j = h[4]
l = (int(i)+int(q)+int(s)+int(g)+int(j))
print(x)
print(b)
print(o)
print(l)
